% integer Bessel functions of odd order
% these are all odd functions so the bounds are exchanged when X < 0
% so limits need to be set separately for positive and negative X
cnf(j1_upper_bound_posX,axiom,
    ( ~ 0 <= X
    | ~ lgen(R,j_14_1(X),Y) | lgen(R,j1(X),Y))).
cnf(j1_lower_bound_posX,axiom,
    ( ~ 0 <= X
    | ~ lgen(R,Y,j_15_1(X)) | lgen(R,Y,j1(X)))).
cnf(j1_upper_bound_negX,axiom,
    ( ~ X <= 0
    | ~ lgen(R,j_15_1(X),Y) | lgen(R,j1(X),Y))).
cnf(j1_lower_bound_negX,axiom,
    ( ~ X <= 0
    | ~ lgen(R,Y,j_14_1(X)) | lgen(R,Y,j1(X)))).

